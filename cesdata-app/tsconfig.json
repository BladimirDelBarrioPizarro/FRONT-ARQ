{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "module": "esnext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "jsx": "react",
    "allowJs": true,
    "strict": false,
    "sourceMap": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declarationDir": "build/types",
    "outDir": "build",
    "typeRoots": [
      "node_modules/@types"
    ],
    "skipLibCheck": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "src",
    "images.d.ts",
    ".eslintrc.js"
  ],
  "exclude": []
}
