{
  "name": "@cesce/ui-catalogue",
  "version": "4.12.2",
  "description": "Catalogo componentes CESDATA Front",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "Atmira Team",
  "scripts": {
    "t": "npx stmux -w always -a a -m beep -M [ [ \"npm run lint:watch\" .. \"npm run start:storybook\" ] : \"npm run test:watch\" ]",
    "ti": "npx stmux -w always -a a -m beep -M [ [ \"npm run lint:watch\" .. \"npm run start:isolated\" ] : \"npm run test:watch\" ]",
    "start": "run-p test:watch lint:watch start:storybook",
    "start:storybook": "start-storybook  -s ./docs/assets",
    "start:isolated": "SKIP_PREFLIGHT_CHECK=true npx forever node_modules/.bin/react-scripts start",
    "start:isolated:nots": "TSC_COMPILE_ON_ERROR=true SKIP_PREFLIGHT_CHECK=true npx forever node_modules/.bin/react-scripts start",
    "build": "run-s clean build:storybook build:lib",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "lint:watch": "watch \"npm run lint\" src/",
    "lint:pro": "eslint \"src/**/*.{js,ts,tsx}\" --quiet ",
    "lint:report": "eslint \"src/**/*.{js,ts,tsx}\" --quiet --cache -f json -o reports/eslintreport.json",
    "test": "run-s lint:report test:coverage",
    "test:watch": "jest --verbose false --watchAll",
    "test:coverage": "jest --coverage",
    "test:nyan": "jest --reporters=\"jest-nyancat-reporter\" --watchAll",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "newcomponent": "plop",
    "build:storybook": "build-storybook -c .storybook -o build",
    "build:lib": "webpack",
    "postbuild:lib": "cp -r src/assets/fonts lib/",
    "clean": "rimraf lib build",
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "release": "standard-version --no-verify",
    "release:dry": "standard-version -v",
    "sonar": "sonar-scanner -Dsonar.sources=src -Dsonar.exclusions=src/__test__/**,demo -Dsonar.tests=src -Dsonar.test.inclusions=src/**/*.test.tsx,src/**/*.test.ts -Dsonar.typescript.lcov.reportPaths=reports/coverage/lcov.info -Dsonar.testExecutionReportPaths=reports/test-reporter.xml -Dsonar.eslint.reportPaths=reports/eslintreport.json -Dsonar.cpd.js.minimumLines=20"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "3.2.10",
    "@material-ui/styles": "4.10.0",
    "@react-pdf-viewer/core": "^2.4.2",
    "@react-pdf-viewer/thumbnail": "^2.4.2",
    "@react-pdf-viewer/toolbar": "^2.4.2",
    "caniuse-lite": "^1.0.30001228",
    "core-js": "3.6.5",
    "fontsource-roboto": "2.2.6",
    "formik": "2.1.7",
    "html2canvas": "^1.0.0-rc.7",
    "identity-obj-proxy": "3.0.0",
    "jspdf": "^2.3.1",
    "mui-datatables": "3.5.0",
    "notistack": "0.9.17",
    "pdfjs-dist": "^2.6.347",
    "react": "16.13.1",
    "react-imask": "6.0.5",
    "react-input-mask": "^2.0.4",
    "react-scripts": "3.4.3",
    "react-text-mask": "5.4.3",
    "react-uuid": "^1.0.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-centered": "5.3.21",
    "@storybook/addon-docs": "5.3.21",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "5.3.21",
    "@storybook/addons": "5.3.21",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "5.3.21",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "babel-loader": "^8.1.0",
    "clean": "4.0.2",
    "cm": "0.3.2",
    "commitizen": "4.2.1",
    "conventional-changelog-cli": "2.1.0",
    "css-loader": "3.6.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "6.8.0",
    "eslint-config-standard-with-typescript": "18.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-watch": "6.0.1",
    "faker": "5.1.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-nyancat-reporter": "2.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "json-server": "0.16.1",
    "lint": "0.7.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "plop": "2.7.4",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.2",
    "semver-utils": "1.1.4",
    "sonarqube-scanner": "2.7.0",
    "standard-version": "9.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "typedoc": "0.19.2",
    "typescript": "3.9.7",
    "watch": "1.0.2",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12"
  }
}
